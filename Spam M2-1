-- Auto Spam M2 (multi-window + auto self-destroy)
-- DÃ¡n vÃ o executor â€” Ä‘á»™c láº­p, khÃ´ng cáº§n GUI hay code gá»‘c

local FIRE_INTERVAL = 0.65 -- khoáº£ng giá»¯a má»—i láº§n gá»­i (giÃ¢y)

-- ğŸ•’ Danh sÃ¡ch cÃ¡c cá»­a sá»• thá»i gian (giÃ¢y tÃ­nh tá»« tick0)
local schedule = {
    {13, 22},   -- báº­t lÃºc 13s, táº¯t lÃºc 22s
    {32, 41},   -- báº­t lÃºc 32s, táº¯t lÃºc 41s
    {51, 55},   -- báº­t lÃºc 51s, táº¯t lÃºc 55s
    {127, 133}, -- báº­t lÃºc 127s, táº¯t lÃºc 133s
}

-- ğŸ” TÃ¬m remote: ReplicatedStorage -> BridgeNet2 -> dataRemoteEvent
local remote
pcall(function()
    local rs = game:GetService("ReplicatedStorage")
    local bridge = rs:FindFirstChild("BridgeNet2")
    if bridge then
        remote = bridge:FindFirstChild("dataRemoteEvent")
    end
    if not remote then
        for _, v in ipairs(rs:GetDescendants()) do
            if v.Name == "dataRemoteEvent" then
                remote = v
                break
            end
        end
    end
end)

-- ğŸ”§ bf(): Æ°u tiÃªn buffer.fromstring náº¿u cÃ³
local bf
if type(buffer) == "table" and type(buffer.fromstring) == "function" then
    bf = buffer.fromstring
else
    bf = function(s) return s end
end

-- ğŸ“¦ Payload M2
local payload_str = "\019\000\000\0000201100101186595482\000"

-- ğŸ“¤ Gá»­i yÃªu cáº§u
local function safeFire()
    if remote and bf then
        pcall(function()
            remote:FireServer({ { bf(payload_str) }, "\019" })
        end)
    else
        if not safeFire._warned then
            safeFire._warned = true
            warn("[Auto Spam M2] remote hoáº·c bf khÃ´ng kháº£ dá»¥ng; khÃ´ng gá»­i Ä‘Æ°á»£c.")
        end
    end
end

-- ğŸ§­ MAIN LOOP
local tick0 = tick()
local finished = 0
local totalSchedules = #schedule

for i, win in ipairs(schedule) do
    local startT, endT = win[1], win[2]
    if type(startT) ~= "number" or type(endT) ~= "number" or endT <= startT then
        warn(("[Auto Spam M2] Schedule #%d khÃ´ng há»£p lá»‡ (start=%s end=%s) â€” bá» qua"):format(i, tostring(startT), tostring(endT)))
    else
        task.spawn(function()
            -- â³ Chá» tá»›i thá»i Ä‘iá»ƒm báº¯t Ä‘áº§u
            local waitUntil = tick0 + startT
            local toWait = waitUntil - tick()
            if toWait > 0 then task.wait(toWait) end

            if tick() >= tick0 + endT then
                warn(("[Auto Spam M2] Schedule #%d Ä‘Ã£ trÃ´i qua trÆ°á»›c khi báº¯t Ä‘áº§u â€” bá» qua."):format(i))
                finished += 1
                return
            end

            print(("[Auto Spam M2] Schedule #%d ğŸŸ¢ Báº¬T (%.1fs â†’ %.1fs)"):format(i, startT, endT))

            -- ğŸ” Gá»­i liÃªn tá»¥c trong khoáº£ng thá»i gian
            while tick() < tick0 + endT do
                safeFire()
                task.wait(FIRE_INTERVAL)
            end

            print(("[Auto Spam M2] Schedule #%d ğŸ”´ Táº®T (%.1fs)"):format(i, endT))

            -- ğŸ§¹ Kiá»ƒm tra náº¿u táº¥t cáº£ Ä‘Ã£ hoÃ n thÃ nh
            finished += 1
            if finished >= totalSchedules then
                print("[Auto Spam M2] âœ… Táº¥t cáº£ schedule Ä‘Ã£ hoÃ n thÃ nh. Äang tá»± dá»n vÃ  xoÃ¡ script...")

                task.delay(1, function()
                    remote, bf, safeFire, schedule = nil, nil, nil, nil
                    table.clear(getfenv())
                    collectgarbage("collect")
                    print("[Auto Spam M2] ğŸ§¹ Script Ä‘Ã£ Ä‘Æ°á»£c tá»± xoÃ¡ hoÃ n toÃ n khá»i bá»™ nhá»›.")
                end)
            end
        end)
    end
end
