local Players = game:GetService("Players")
local p = Players.LocalPlayer
local running = true

-- ğŸ•’ AUTO TELE "Cá»•ng 1" SAU 3 GIÃ‚Y Rá»’I Táº®T SAU 1 GIÃ‚Y
task.spawn(function()
	task.wait(3)
	if not running then return end
	print("[AUTO] Dá»‹ch chuyá»ƒn tá»›i Cá»•ng 1 sau 3 giÃ¢y")

	-- Vá»‹ trÃ­ cá»•ng 1
	local v = Vector3.new(18.86713409423828, 0.37274569272994995, -114.86202239990234)

	local chr = p.Character or p.CharacterAdded:Wait()
	local hrp = chr and chr:FindFirstChild("HumanoidRootPart")
	if hrp then
		-- Dá»‹ch chuyá»ƒn nhÃ¢n váº­t lÃªn 5 Ä‘Æ¡n vá»‹ Ä‘á»ƒ trÃ¡nh káº¹t
		hrp.CFrame = CFrame.new(v + Vector3.new(0, 5, 0))
	end

	-- Giá»¯ trong 1 giÃ¢y rá»“i dá»«ng
	task.wait(1)
	if not running then return end
	print("[AUTO] Dá»«ng teleport sau 1 giÃ¢y")
end)

-- â™»ï¸ Tá»° Äá»˜NG CLEAR SCRIPT SAU 140 GIÃ‚Y (hoÃ n toÃ n Ä‘á»™c láº­p)
task.spawn(function()
	task.wait(140)
	running = false
	print("[AUTO] Script Auto Tele Cá»•ng 1 Ä‘Ã£ tá»± Ä‘á»™ng clear sau 140 giÃ¢y")
	pcall(function()
		script:Destroy()
	end)
end)
