-- ‚úÖ PH·∫¶N TH·ª∞C T·∫æ: AUTO B·∫¨T TP2 LOCK SAU 7 GI√ÇY
local Players = game:GetService("Players")
local p = Players.LocalPlayer

local STATE = {tp1 = false}

task.spawn(function()
	task.wait(7)
	print("[AUTO] B·∫≠t TP1 Lock sau 7 gi√¢y")

	STATE.tp1 = true
	local pos = Vector3.new(40,3.63,-130.14)
	local look
	pcall(function()
		look = p.Character 
			and p.Character:FindFirstChild("HumanoidRootPart") 
			and p.Character.HumanoidRootPart.CFrame.LookVector
	end)

	task.spawn(function()
		while STATE.tp1 do
			task.wait()
			local c = p.Character
			if c and c:FindFirstChild("HumanoidRootPart") then
				local h = c.HumanoidRootPart
				h.Velocity = Vector3.new()
				if look then
					h.CFrame = CFrame.new(pos, pos + look)
				else
					h.CFrame = CFrame.new(pos)
				end
			end
		end
	end)
end)
