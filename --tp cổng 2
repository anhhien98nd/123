-- ==============================================
-- âœ… PHáº¦N CODE CHáº Y THá»°C Táº¾: AUTO TELEPORT (Cá»•ng 2)
-- ==============================================

local Players = game:GetService("Players")
local p = Players.LocalPlayer
local running = true

-- ğŸ•’ AUTO TELE "Cá»•ng 2" SAU 3 GIÃ‚Y Rá»’I Táº®T SAU 1 GIÃ‚Y
task.spawn(function()
	task.wait(3)
	if not running then return end
	print("[AUTO] Dá»‹ch chuyá»ƒn tá»›i Cá»•ng 2 sau 3 giÃ¢y")

	-- Vá»‹ trÃ­ Cá»•ng 2
	local v = Vector3.new(18.37334884643555, 0.37274569272994995, -140.72775268554688)

	local chr = p.Character or p.CharacterAdded:Wait()
	local hrp = chr and chr:FindFirstChild("HumanoidRootPart")
	if hrp then
		-- Dá»‹ch chuyá»ƒn lÃªn má»™t chÃºt Ä‘á»ƒ trÃ¡nh káº¹t map
		hrp.CFrame = CFrame.new(v + Vector3.new(0, 5, 0))
	end

	-- Giá»¯ trong 1 giÃ¢y rá»“i dá»«ng
	task.wait(1)
	if not running then return end
	print("[AUTO] Dá»«ng teleport sau 1 giÃ¢y")
end)

-- â™»ï¸ Tá»° Äá»˜NG CLEAR SCRIPT SAU 140 GIÃ‚Y (Ä‘á»™c láº­p hoÃ n toÃ n)
task.spawn(function()
	task.wait(140)
	running = false
	print("[AUTO] Script Auto Tele Cá»•ng 2 Ä‘Ã£ tá»± Ä‘á»™ng clear sau 140 giÃ¢y")
	pcall(function()
		script:Destroy()
	end)
end)
